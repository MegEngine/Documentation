msgid "megengine.functional.normalize"
msgstr "crwdns31580:0crwdne31580:0"

#: megengine.functional.math.normalize:1 of
msgid "Performs :math:`L_p` normalization of input tensor along given axis."
msgstr "crwdns31582:0:math:crwdne31582:0"

#: megengine.functional.math.normalize:4 of
msgid "For a tensor of shape :math:`(n_0, ..., n_{dim}, ..., n_k)`, each :math:`n_{dim}` -element vector :math:`v` along dimension :attr:`axis` is transformed as:"
msgstr "crwdns31584:0:math:crwdnd31584:0{dim}crwdnd31584:0:math:crwdnd31584:0{dim}crwdnd31584:0:math:crwdnd31584:0:attr:crwdne31584:0"

#: megengine.functional.math.normalize:7 of
msgid "v = \\frac{v}{\\max(\\lVert v \\rVert_p, \\epsilon)}.\n\n"
msgstr "crwdns31586:0{v}crwdne31586:0"

#: megengine.functional.math.normalize of
msgid "参数"
msgstr "crwdns31588:0crwdne31588:0"

#: megengine.functional.math.normalize:11 of
msgid "input tensor."
msgstr "crwdns31590:0crwdne31590:0"

#: megengine.functional.math.normalize:13 of
msgid "power of value applied to input tensor. Default: 2"
msgstr "crwdns31592:0crwdne31592:0"

#: megengine.functional.math.normalize:15 of
msgid "dimension to reduce.If None, input must be a vector. Default: None"
msgstr "crwdns31594:0crwdne31594:0"

#: megengine.functional.math.normalize:17 of
msgid "a small value to avoid division by zero. Default: 1e-12"
msgstr "crwdns31596:0crwdne31596:0"

#: megengine.functional.math.normalize of
msgid "返回类型"
msgstr "crwdns31598:0crwdne31598:0"

#: megengine.functional.math.normalize:18 of
msgid ":py:class:`~megengine.tensor.Tensor`"
msgstr "crwdns31600:0crwdne31600:0"

#: megengine.functional.math.normalize of
msgid "返回"
msgstr "crwdns31602:0crwdne31602:0"

#: megengine.functional.math.normalize:19 of
msgid "normalized output tensor."
msgstr "crwdns31604:0crwdne31604:0"

