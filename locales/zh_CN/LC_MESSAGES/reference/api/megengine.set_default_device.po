msgid "megengine.set\\_default\\_device"
msgstr "megengine.set\\_default\\_device"

#: megengine.device.set_default_device:1 of
msgid "Sets default computing node."
msgstr "设置默认的计算节点。"

#: megengine.device.set_default_device of
msgid "参数"
msgstr ""

#: megengine.device.set_default_device:4 of
msgid "default device type."
msgstr ""

#: megengine.device.set_default_device:8 of
msgid "The type can be 'cpu0', 'cpu1', etc., or 'gpu0', 'gpu1', etc., to specify the particular CPU or GPU to use."
msgstr ""

#: megengine.device.set_default_device:10 of
msgid "'cpux' and  'gpux' can also be used to specify any number of CPU or GPU devices."
msgstr ""

#: megengine.device.set_default_device:11 of
msgid "The default value is 'xpux' to specify any device available."
msgstr ""

#: megengine.device.set_default_device:12 of
msgid "The priority of using GPU is higher when both GPU and CPU are available."
msgstr ""

#: megengine.device.set_default_device:13 of
msgid "'multithread' device type is avaliable when inference, which implements multi-threading parallelism at the operator level. For example, 'multithread4' will compute with 4 threads."
msgstr ""

#: megengine.device.set_default_device:16 of
msgid "It can also be set by environment variable ``MGE_DEFAULT_DEVICE``."
msgstr ""

