   "source": [
    "from megengine import tensor\n",
    "import megengine.functional as F\n",
    "\n",
    "x = tensor(np.random.random((10, 28, 28, 1)))\n",
    "out = F.flatten(x, start_axis=1, end_axis=-1)  # 将从 start_axis 维到 end_axis 维的子张量展平\n",
    "print(x.shape)\n",
    "print(out.shape)"
   ]
